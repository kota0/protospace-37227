<main class="main">
  <div class="inner">
    <%# ログインしているときは以下を表示する %>
    <% if user_signed_in? %>
      <div class="greeting">
        <%= link_to "こんにちは" "#{current_user.name}さん", user_path(current_user), class: :greeting__link %>
      </div> 
    <% end %> 
    <%# // ログインしているときは上記を表示する %>
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @prototypes %>
    </div>
    <ul class="more_list">
       <li>
        <%= link_to '詳細' , prototype_path(@prototypes.ids) %>
       </li>
    </ul>
  </div>
</main>
